@model MVC_Web_App.Models.Category

@{
    ViewData["Title"] = "حذف تصنيف اساسى";
}
<div class="container">
    <div class="page-title">
        <h4> حذف تصنيف اساسى</h4>
    </div>

    <h3>هل انت متأكد انك تريد حذف هذا التصنيف ؟</h3>
    <div>
        <h4>Category</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CatName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CatName)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="حذف" class="btn btn-danger" /> |
            <a asp-action="Index">العودة الى قائمة التصنيفات</a>
        </form>
    </div>
</div>