@model MVC_Web_App.Models.SubCategory

@{
    ViewData["Title"] = "حذف التصنيفات الفرعية";
}
<div class="container">
    <div class="page-title">
        <h3 class="text-dark">حذف تصنيف فرعى</h3>
    </div>



    <h5 class="text-danger">هل انت متأكد من حذف التصنيف الفرعى ؟</h5>
    <div>

        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SubCatName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SubCatName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Category)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Category.CatName)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="حذف" class="btn btn-danger" /> |
            <a asp-action="Index">العودة الى قائمة التصنيفات الفرعية</a>
        </form>
    </div>
</div>