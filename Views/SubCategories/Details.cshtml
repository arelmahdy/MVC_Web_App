@model MVC_Web_App.Models.SubCategory

@{
    ViewData["Title"] = "تفاصيل التصنيفات الفرعية";
}
<div class="container">
    <div class="page-title">
        <h3 class="text-info">التفاصيل لتصنيف فرعى</h3>
    </div>



    <div>
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.SubCatName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.SubCatName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Category)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Category.CatName)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id">تعديل</a> |
        <a asp-action="Index">العودة الى قائمة التصنيفات الفرعية</a>
    </div>
</div>